{
  "name": "@open-credit-cards/mcp-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "embed-data": "bun run scripts/embed-data.ts",
    "build": "bun run embed-data && bun build src/index.ts --outdir dist --target browser --minify",
    "test": "bun run embed-data && bun test",
    "dev": "bun run embed-data && wrangler dev",
    "deploy": "bun run build && wrangler deploy"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "1.26.0",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20260213.0",
    "@types/bun": "1.3.9",
    "ajv": "8.17.1",
    "json-schema-to-typescript": "15.0.4",
    "wrangler": "4.65.0"
  }
}
